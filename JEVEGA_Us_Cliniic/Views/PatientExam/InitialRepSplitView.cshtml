        $('#btnLoadReport').on({
            'click': function () {

                var template_index = $('#ReportTemplateList').find(":selected").val();
                if (template_index == 0) { alert("Please select a template ..."); }

                $.ajax({
                    url: '/PatientExam/loadReportTemplate/' + template_index.toString(),
                    type: 'GET',
                    success: function (report_template_data) {
                        $("#ExamReport").append(report_template_data);
                    },
                    error: function (error) {
                        $(that).remove();
                        DisplayError(error.statusText);
                    }
                });

            }
        });

        $("#btnClearReport").on({
            'click': function () {
                $("#ExamReport").empty();
            }
        });

